{
  "name": "pt-plugin-plus",
  "version": "1.4.2",
  "author": {
    "name": "ronggang",
    "url": "https://github.com/ronggang"
  },
  "displayName": "PT 助手 Plus",
  "homepage": "https://github.com/ronggang/PT-Plugin-Plus",
  "scripts": {
    "serve": "vue-cli-service serve --mode=test",
    "build": "yarn install && vue-cli-service build && webpack --config webpack/prod-background.js && webpack --config webpack/prod-content.js && cd debug && yarn install && tsc && node ./dist/buildResource.js && cd .. && web-ext build -s ./dist -a ./releases -o",
    "lint": "vue-cli-service lint",
    "background": "webpack --config webpack/prod-background.js && webpack --config webpack/prod-content.js",
    "dev-background": "webpack --config webpack/dev-background.js && webpack --config webpack/dev-content.js",
    "dev": "yarn install && vue-cli-service build --mode=development && webpack --config webpack/dev-background.js && webpack --config webpack/dev-content.js && cd debug && yarn install && tsc && node ./dist/buildResource.js",
    "dev-s": "cd debug && yarn install && tsc && node ./dist/index.js",
    "resource": "cd debug && yarn install && tsc && node ./dist/buildResource.js"
  },
  "dependencies": {
    "basiccontext": "^3.5.1",
    "blueimp-md5": "^2.10.0",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.8.11",
    "extend": "^3.0.2",
    "file-saver": "^2.0.0",
    "highcharts": "^7.1.1",
    "highcharts-vue": "^1.2.0",
    "html2canvas": "^1.0.0-alpha.12",
    "i18next": "^17.0.3",
    "jszip": "^3.2.2",
    "parse-torrent": "^7.0.1",
    "ua-parser-js": "^0.7.20",
    "url-parse": "^1.4.4",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-i18n": "^8.11.2",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1",
    "webdav": "^2.10.0"
  },
  "devDependencies": {
    "@types/blueimp-md5": "^2.7.0",
    "@types/chrome": "^0.0.75",
    "@types/crypto-js": "^3.1.43",
    "@types/extend": "^3.0.0",
    "@types/file-saver": "^2.0.0",
    "@types/html2canvas": "^0.0.35",
    "@types/jquery": "^3.3.22",
    "@types/jszip": "^3.1.6",
    "@types/parse-torrent": "^5.8.3",
    "@types/ua-parser-js": "^0.7.33",
    "@types/url-parse": "^1.4.2",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.2.0",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-typescript": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "terser-webpack-plugin": "^2.2.1",
    "ts-loader": "^5.3.1",
    "typescript": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5",
    "web-ext": "^3.0.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "typescript-eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}